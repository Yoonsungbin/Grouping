<!DOCTYPE html>
<html>
<head>
	<title></title>
 <script src="../javascripts/filelist.js"></script>
 <script src="http://code.jquery.com/jquery-latest.js"></script>
</head>
<body>
<form action="upload" method="POST" enctype="multipart/form-data">
    <input type="file" name="drive" /><br/>
    <input type="submit" />
</form>
<script>
$.getJSON('/showlist',function(data ) {
for(var item in data){
            tableContent += '<tr>';
            count+= 1;
            tableContent += '<td>'+count+'  :  '+'</td>';
            tableContent += '<td><a href='+'/'+data[item]+'>'+data[item]+'</a></td>';
        }
        $('#adownload').html(tableContent);

});
</script>

<div id= "adownload">
	<table></table>
</div>
</body>
</html>
